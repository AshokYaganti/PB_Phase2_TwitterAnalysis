<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<display-name>TwitterAnalysis</display-name>
	<welcome-file-list>
		<welcome-file>Home.html</welcome-file>
	</welcome-file-list>
	
	<servlet>
        <servlet-name>TwitterAnalysisQueries</servlet-name>
        <servlet-class>com.umkc.ashok.TwitterAnalysisQueries</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>TwitterAnalysisQueries</servlet-name>
        <url-pattern>/TwitterAnalysisQueries</url-pattern>
    </servlet-mapping>
    
</web-app>